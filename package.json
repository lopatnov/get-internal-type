{
  "name": "@lopatnov/get-internal-type",
  "version": "2.2.0",
  "description": "Determine the internal JavaScript [[Class]] of an object. A lightweight, cross-platform TypeScript library for reliable runtime type detection.",
  "author": {
    "name": "lopatnov",
    "url": "https://github.com/lopatnov"
  },
  "license": "Apache-2.0",
  "homepage": "https://lopatnov.github.io/get-internal-type/",
  "source": "src/get-internal-type.ts",
  "main": "dist/get-internal-type.cjs",
  "module": "dist/get-internal-type.esm.mjs",
  "browser": "dist/get-internal-type.umd.js",
  "types": "dist/get-internal-type.d.ts",
  "exports": {
    ".": {
      "types": "./dist/get-internal-type.d.ts",
      "import": "./dist/get-internal-type.esm.mjs",
      "require": "./dist/get-internal-type.cjs",
      "browser": "./dist/get-internal-type.umd.js",
      "default": "./dist/get-internal-type.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "umdName": "getInternalType",
  "keywords": [
    "type-detection",
    "type-checking",
    "typeof",
    "internal-class",
    "object-type",
    "runtime-type",
    "javascript",
    "typescript",
    "cross-browser",
    "node",
    "lightweight",
    "utility"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "watch": "rollup -cw --config rollup.config.mjs",
    "test": "jest  --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.{ts,js}\"",
    "configure-npm": "node ./set-registry.js -s https://registry.npmjs.org",
    "configure-gpr": "node ./set-registry.js -s https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopatnov/get-internal-type.git"
  },
  "bugs": {
    "url": "https://github.com/lopatnov/get-internal-type/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@lopatnov/rollup-plugin-uglify": "^4.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.0.0",
    "jest-config": "^30.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.55.0",
    "terser": "^5.37.0",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.55.0"
  }
}
